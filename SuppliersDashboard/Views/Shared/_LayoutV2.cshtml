        }

        .navbar-nav .nav-link {
            font-size: 0.875rem;
        }

            .navbar-nav .nav-link::before {
                content: none !important; /* disables any default markers */
            }

        .navbar-vertical .navbar-nav .nav-link[data-bs-toggle="collapse"]::after {
            content: "▼"; /* Simple Unicode down arrow */
            font-size: 0.75rem;
            display: inline-block;
            margin-left: inherit;
            color: transparent; /* Make text color transparent */
            background-color: #f97316; /* Gradient background */
            -webkit-background-clip: text; /* Apply gradient to text */
            background-clip: text;
            padding-right: 10px;
            transition: transform 0.3s ease;
        }

        .navbar-vertical .navbar-nav .nav-link[aria-expanded="true"]::after {
            transform: rotate(180deg); /* Flip arrow when open */
        }
        /* تحديد العرض الأساسي للشريط الجانبي */
        .sidenav {
            width: 250px; /* العرض الأساسي للشريط */
            transition: width 0.3s ease; /* جعل التغيير في العرض يتم بسلاسة */
        }

            /* زيادة العرض عند المرور بالماوس */
            .sidenav:hover {
                width: 300px; /* العرض عند المرور بالماوس */
            }

        /* النص داخل العناصر */
        .navbar-vertical .navbar-nav .nav-item .nav-link {
            white-space: nowrap; /* منع النص من الالتفاف */
            overflow: hidden; /* إخفاء النص الزائد */
            text-overflow: ellipsis; /* إظهار ثلاث نقاط عند قص النص */
            display: block;
            width: 100%; /* ضمان أن العرض يأخذ كامل عرض العنصر */
        }


            /* عند المرور بالماوس على العنصر لعرض النص الكامل */
            .navbar-vertical .navbar-nav .nav-item .nav-link:hover {
                white-space: normal; /* السماح للنص بالالتفاف عند المرور بالماوس */
                background-color: rgba(0, 0, 0, 0.1); /* تغيير اللون عند المرور بالماوس */
                transition: background-color 0.3s ease; /* جعل الانتقال للون يتم بسلاسة */
                overflow: visible; /* عرض النص بالكامل عند المرور */
            }

        /* لتنسيق العناصر الفرعية (Sub-items) */
        .navbar-vertical .navbar-nav .nav-item .collapse .nav-item .nav-link {
            white-space: nowrap; /* منع التفاف النص في العناصر الفرعية */
